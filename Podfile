# platform :ios, '9.0'

target 'Pinder' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Pinder

pod 'Firebase/Core', '5.20.1'
pod 'Firebase/Auth', '5.20.1'
