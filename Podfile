pod 'Firebase/Storage', '5.20.1'
pod 'Firebase/Database', '5.20.1'

  target 'PinderTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PinderUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
